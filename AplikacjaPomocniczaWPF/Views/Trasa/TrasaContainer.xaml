<UserControl x:Class="AplikacjaPomocniczaWPF.Views.Trasa.TrasaContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AplikacjaPomocniczaWPF.Views.Trasa"
             xmlns:vc="clr-namespace:AplikacjaPomocniczaWPF.ValueConverters"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl Content="{Binding CurrentPage, Converter={vc:PageTypeToUserControlValueConverter}}" />
</UserControl>
